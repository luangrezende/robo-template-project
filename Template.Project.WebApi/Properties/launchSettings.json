{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:3001",
      "sslPort": 0
    }
  },

  "$schema": "http://json.schemastore.org/launchsettings.json",

  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "api/template/swagger",
      "environmentVariables": {
        "DATABASE_URL": "Server=(localdb)\\mssqllocaldb;Database=Api.Template;Integrated Security=True",
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },

    "Template.Project": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "api/template/swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:5000"
    },

    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/template/swagger",
      "environmentVariables": {
        "ASPNETCORE_URLS": "http://+:3001"
      },
      "httpPort": 3001,
      "publishAllPorts": true
    },

    "Container in Kubernetes": {
      "commandName": "AzureDevSpaces",
      "launchBrowser": true
    }
  }
}